<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>UserAccountController</servlet-name>
        <servlet-class>controller.home.UserAccountController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterController</servlet-name>
        <servlet-class>controller.auth.RegisterController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerificationController</servlet-name>
        <servlet-class>controller.auth.VerificationController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ForgotPasswordController</servlet-name>
        <servlet-class>controller.auth.ForgotPasswordController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewListController</servlet-name>
        <servlet-class>controller.home.NewListController</servlet-class>
    </servlet>
   <servlet>
        <servlet-name>NewDetailController</servlet-name>
        <servlet-class>controller.home.NewDetailController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>controller.auth.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeController</servlet-name>
        <servlet-class>controller.home.HomeController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManageDoctorController</servlet-name>
        <servlet-class>controller.manager.ManageDoctorController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServiceController</servlet-name>
        <servlet-class>controller.home.ServiceController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManageBlogController</servlet-name>
        <servlet-class>controller.manager.ManageBlogController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SettingList</servlet-name>
        <servlet-class>controller.setting.SettingList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddSetting</servlet-name>
        <servlet-class>controller.setting.AddSetting</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServiceDetailController</servlet-name>
        <servlet-class>controller.home.ServiceDetailController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WebDoctorList</servlet-name>
        <servlet-class>controller.manager.WebDoctorList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserAccountController</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterController</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerificationController</servlet-name>
        <url-pattern>/verify</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ForgotPasswordController</servlet-name>
        <url-pattern>/forgot</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewListController</servlet-name>
        <url-pattern>/new</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewDetailController</servlet-name>
        <url-pattern>/newdetail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeController</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManageDoctorController</servlet-name>
        <url-pattern>/manage-doctor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServiceController</servlet-name>
        <url-pattern>/service</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SettingList</servlet-name>
        <url-pattern>/settinglist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServiceDetailController</servlet-name>
        <url-pattern>/servicedetail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddSetting</servlet-name>
        <url-pattern>/addSetting</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WebDoctorList</servlet-name>
        <url-pattern>/WebDoctorList</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
